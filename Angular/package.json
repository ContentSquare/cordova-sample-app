{
  "name": "cordova-angular-sample-app",
  "version": "0.2.0",
  "author": "Contentsquare Mobile Team",
  "homepage": "https://contentsquare.com/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "add-url-scheme-qa": "ionic cordova plugin add cordova-plugin-customurlscheme --variable URL_SCHEME=cs-com.contentsquare.sampleapp.cordova.angular.qa",
    "add-url-scheme": "ionic cordova plugin add cordova-plugin-customurlscheme --variable URL_SCHEME=cs-com.contentsquare.sampleapp.cordova.angular",
    "build-ios-qa": "gulp cordova-angular-sample-app-qa && ionic cordova platform rm ios && npm run add-url-scheme-qa && ionic cordova platform add ios && ionic cordova build ios --device --debug --buildConfig",
    "build-ios": "gulp cordova-angular-sample-app && ionic cordova platform rm ios && npm run add-url-scheme && ionic cordova platform add ios && ionic cordova build ios --emulator --release --prod --buildConfig",
    "build-android-qa": "gulp cordova-angular-sample-app-qa && ionic cordova platform rm android && npm run add-url-scheme-qa && ionic cordova platform add android && ionic cordova build android --debug --buildConfig",
    "build-android": "gulp cordova-angular-sample-app && ionic cordova platform rm android && npm run add-url-scheme && ionic cordova platform add android && ionic cordova build android --release --prod --buildConfig"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^14.0.0",
    "@angular/core": "^14.0.0",
    "@angular/forms": "^14.0.0",
    "@angular/platform-browser": "^14.0.0",
    "@angular/platform-browser-dynamic": "^14.0.0",
    "@angular/router": "^14.0.0",
    "@ionic/angular": "^6.1.9",
    "@ionic/cordova-builders": "^7.0.0",
    "ionicons": "^6.0.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.0.0",
    "@angular-eslint/builder": "~13.0.1",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "14.2.4",
    "@angular/compiler": "^14.0.0",
    "@angular/compiler-cli": "^14.0.0",
    "@angular/language-service": "^14.0.0",
    "@contentsquare/cordova-plugin": "^0.2.0",
    "@contentsquare/cordova-plugin-types": "^0.2.1",
    "@ionic/angular-toolkit": "^6.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "cordova-android": "^10.1.2",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-add-swift-support": "^2.0.2",
    "cordova-plugin-customurlscheme": "^5.0.2",
    "cordova-plugin-device": "2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.0.5",
    "cordova-plugin-ionic-webview": "^5.0.0",
    "cordova-plugin-splashscreen": "5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "gulp": "^4.0.2",
    "gulp-util": "^3.0.8",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.7.3",
    "xmldoc": "^1.2.0"
  },
  "description": "A sample app Ionic/Angular/Cordova. Implements the basic functionalities of the Contentsquare Cordova plugin",
  "cordova": {
    "plugins": {
      "@contentsquare/cordova-plugin": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "cs-com.contentsquare.sampleapp.cordova.angular"
      }
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}